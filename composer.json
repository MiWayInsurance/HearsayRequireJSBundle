{
    "name": "hearsay/require-js-bundle",
    "description": "This bundle provides integration of the RequireJS library into Symfony2",
    "type": "symfony-bundle",
    "keywords": [
        "amd",
        "commonjs",
        "requirejs",
        "rjs"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Montag",
            "email": "kmontag@cs.stanford.edu"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/finder": ">=2.0",
        "symfony/yaml": ">=2.0",
        "symfony/assetic-bundle": "^2.8",
        "symfony/framework-bundle": "^2.3|^3.0",
        "symfony/templating": "^3.3"
    },
    "require-dev": {
        "twig/extensions": "v1.0.0",
        "squizlabs/php_codesniffer": "^2.7",
        "phpunit/phpunit": "^5.7",
        "phpunit/phpunit-mock-objects":"^3.4",
        "phpro/grumphp": "^0.11.1"
    },
    "autoload": {
        "psr-0": {
            "Hearsay\\RequireJSBundle": ""
        }
    },
    "target-dir": "Hearsay/RequireJSBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
